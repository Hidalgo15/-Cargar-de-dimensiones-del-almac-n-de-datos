    {
        string connectionString = "Data Source=REDACTED;Initial Catalog=REDACTED;User ID=REDACTED;Password=REDACTED";
      
        try
        {
            using (SqlConnection connection = new SqlConnection(connectionString))
            {
                await connection.OpenAsync();
                using (SqlCommand command = new SqlCommand("INSERT INTO dbo.Customers (CustomerId,CustomerContactName, CustomerCity,CustomerRegion, CustomerCountry) " +
                    "VALUES (@CustomerId, @CustomerContactName, @CustomerCity, @CustomerRegion, @CustomerCountry)", connection))
                {
                    command.Parameters.AddWithValue("@CustomerId", CustomerId);
                    command.Parameters.AddWithValue("@CustomerName", CustomerContactName);
                    command.Parameters.AddWithValue("@CustomerRegion", CustomerRegion);
                    command.Parameters.AddWithValue("@CustomerCity", CustomerCity);
                    command.Parameters.AddWithValue("@CustomerCountry", CustomerCountry);
                    
                    command.CommandType = CommandType.Text;

                    await command.ExecuteNonQueryAsync();
                }
            }
        }
        catch (Exception ex)
        {
            throw ex;
        }


    }

