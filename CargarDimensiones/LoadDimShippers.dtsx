    {
        string connectionString = "Data Source=REDACTED;Initial Catalog=REDACTED;User ID=REDACTED;Password=REDACTED";

        try
        {
            using (SqlConnection connection = new SqlConnection(connectionString))
            {
                await connection.OpenAsync();
                using (SqlCommand command = new SqlCommand("INSERT INTO dbo.Shippers (ShipperId,NombreTransportista)" +
                  "Values(@ShipperID,@NombreTransportista)", connection))
                {
                    command.Parameters.AddWithValue("@ShipperID", ShipperId);
                    command.Parameters.AddWithValue("@NombreTransportista", NombreTransportista);
                    command.Parameters.AddWithValue("@TotalVentas", TotalVentas);

                    command.CommandType = CommandType.Text;

